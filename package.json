{
  "name": "srs1-cch",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "meteor-node-stubs": "~0.2.0",
    "react": "^15.3.0",
    "react-addons-pure-render-mixin": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-mounter": "^1.2.0",
    "viewmodel-react": "^0.6.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "viewmodel-react-plugin": "^0.6.0"
  },
  "babel": {
		"presets": ["es2015", "react"],
		"plugins": [
			["viewmodel-react-plugin", {
				"attributes": [
					"id",
					"src", "alt",
					"href", "title", "target",
					"type", "placeholder", "autocomplete",
					"rows", "cols",

					"xlinkHref",
					"viewBox", "width", "height",
					"transform", "x", "y", "cx", "cy", "x1", "y1", "x2", "y2",
					"r", "rx", "ry",
					"d", "points",
					"fill", "fillOpacity", "fillRule",
					"stroke", "strokeOpacity", "strokeWidth",
					"fontSize", "textAnchor", "alignmentBaseline"
				]
			}]
		]
	},
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  }
}
